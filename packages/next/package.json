{
  "name": "@cossistant/next",
  "version": "0.0.3",
  "private": false,
  "author": "Cossistant team",
  "description": "Next.js-ready SDK for building AI-powered support/chat widgets. Hooks + primitives, WS-driven, TypeScript-first. Next.js-ready, Tailwind optional.",
  "keywords": [
    "react",
    "react-hooks",
    "react-components",
    "headless",
    "headless-ui",
    "support",
    "customer-support",
    "helpdesk",
    "support-widget",
    "chat-widget",
    "live-chat",
    "ai",
    "ai-support",
    "ai-agent",
    "support-agent",
    "sdk",
    "typescript",
    "nextjs",
    "websocket",
    "tanstack-query",
    "open-source",
    "tailwind",
    "shadcn",
    "a11y",
    "accessibility"
  ],
  "bugs": {
    "url": "https://github.com/cossistantcom/cossistant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cossistantcom/cossistant.git",
    "directory": "packages/next"
  },
  "license": "MIT",
  "homepage": "https://cossistant.com",
  "exports": {
    ".": "./src/index.ts",
    "./primitives": "./src/primitives/index.ts",
    "./support": "./src/support/index.tsx",
    "./hooks": "./src/hooks/index.ts",
    "./hooks/*": "./src/hooks/*",
    "./support.css": "./src/support/support.css",
    "./realtime": "./src/realtime/index.ts",
    "./utils": "./src/utils/index.ts",
    "./utils/*": "./src/utils/*.ts"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "dependencies": {
    "@cossistant/react": "workspace:*",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@cossistant/typescript-config": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18 <20",
    "react-dom": ">=18 <20",
    "next": ">=13.5.2 || ^14.0.0 || ^15.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "tsdown && bun run prepare-package",
    "prepare-package": "bunx tsx ../../scripts/prepare-package.ts",
    "lint": "biome check .",
    "check-types": "tsc --noEmit --project tsconfig.json"
  }
}
