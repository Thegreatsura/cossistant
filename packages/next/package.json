{
  "name": "@cossistant/next",
  "version": "0.0.0",
  "private": false,
  "author": "Cossistant team",
  "description": "Next.js-ready SDK for building AI-powered support/chat widgets. Hooks + primitives, WS-driven, TypeScript-first. Next.js-ready, Tailwind optional.",
  "keywords": [
    "react",
    "react-hooks",
    "react-components",
    "headless",
    "headless-ui",
    "support",
    "customer-support",
    "helpdesk",
    "support-widget",
    "chat-widget",
    "live-chat",
    "ai",
    "ai-support",
    "ai-agent",
    "support-agent",
    "sdk",
    "typescript",
    "nextjs",
    "websocket",
    "tanstack-query",
    "open-source",
    "tailwind",
    "shadcn",
    "a11y",
    "accessibility"
  ],
  "bugs": {
    "url": "https://github.com/cossistantcom/cossistant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cossistantcom/cossistant.git",
    "directory": "packages/next"
  },
  "license": "MIT",
  "homepage": "https://cossistant.com",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "import": "./dist/primitives/index.js"
    },
    "./support": {
      "types": "./dist/support/index.d.ts",
      "import": "./dist/support/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./hooks/*": {
      "types": "./dist/hooks/*.d.ts",
      "import": "./dist/hooks/*.js"
    },
    "./realtime": {
      "types": "./dist/realtime/index.d.ts",
      "import": "./dist/realtime/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cossistant/react": "workspace:*",
    "tailwindcss": "^4.1.13"
  },
  "peerDependencies": {
    "react": ">=18 <20",
    "react-dom": ">=18 <20",
    "next": ">=13.5.2 || ^14.0.0 || ^15.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "tsdown",
    "lint": "biome check .",
    "check-types": "tsc --noEmit --project tsconfig.json"
  }
}
