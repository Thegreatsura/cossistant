{
  "name": "@cossistant/micro-react-avatars",
  "version": "1.0.0",
  "private": false,
  "author": "Cossistant team",
  "description": "A lightweight, customizable React avatar library with predictable generation based on Avvvatars",
  "keywords": [
    "react",
    "avatar",
    "avatar-generator",
    "profile-picture",
    "identicon",
    "react-component",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/cossistantcom/cossistant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cossistantcom/cossistant.git",
    "directory": "packages/micro-react-avatars"
  },
  "license": "MIT",
  "homepage": "https://cossistant.com",
  "exports": {
    ".": {
      "types": "./src/index.tsx",
      "import": "./src/index.tsx",
      "require": "./src/index.tsx"
    }
  },
  "main": "./src/index.tsx",
  "module": "./src/index.tsx",
  "types": "./src/index.tsx",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@cossistant/typescript-config": "workspace:*",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  }
}