{
  "name": "@cossistant/react",
  "version": "0.0.0",
  "private": false,
  "author": "Cossistant team",
  "description": "Headless React SDK for building AI-powered support/chat widgets. Hooks + primitives, WS-driven, TypeScript-first. Next.js-ready, Tailwind optional.",
  "keywords": [
    "react",
    "react-hooks",
    "react-components",
    "headless",
    "headless-ui",
    "support",
    "customer-support",
    "helpdesk",
    "support-widget",
    "chat-widget",
    "live-chat",
    "ai",
    "ai-support",
    "ai-agent",
    "support-agent",
    "sdk",
    "typescript",
    "nextjs",
    "websocket",
    "tanstack-query",
    "open-source",
    "tailwind",
    "shadcn",
    "a11y",
    "accessibility"
  ],
  "bugs": {
    "url": "https://github.com/cossistantcom/cossistant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cossistantcom/cossistant.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "homepage": "https://cossistant.com",
  "exports": {
    ".": "./src/index.ts",
    "./primitives": "./src/primitives/index.ts",
    "./primitives/*": "./src/primitives/*.ts",
    "./support": "./src/support/index.tsx",
    "./hooks": "./src/hooks/index.ts",
    "./hooks/*": "./src/hooks/*.ts",
    "./support.css": "./src/support/support.css",
    "./realtime": "./src/realtime/index.ts",
    "./utils": "./src/utils/index.ts",
    "./utils/*": "./src/utils/*.ts"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsdown && bun run build:css",
    "build:css": "bunx @tailwindcss/cli -i src/support/support.css -o dist/support.css --content 'src/**/*.{js,ts,jsx,tsx}' --minify"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@cossistant/core": "workspace:*",
    "@cossistant/types": "workspace:*",
    "nanoid": "^5.1.5",
    "react-markdown": "^10.1.0",
    "react-use-websocket": "^4.13.0",
    "tailwind-merge": "^3.3.1",
    "ulid": "^3.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "postcss": "^8.5.6",
    "@cossistant/typescript-config": "workspace:*",
    "autoprefixer": "^10.4.21",
    "@tanstack/react-query": "^5.89.0"
  },
  "peerDependencies": {
    "react": ">=18 <20",
    "react-dom": ">=18 <20",
    "@types/react": "^18 || ^19",
    "@tanstack/react-query": "^5.0.0",
    "motion": "^12.18.1",
    "tailwindcss": "*"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
