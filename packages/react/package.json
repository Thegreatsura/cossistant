{
  "name": "@cossistant/react",
  "version": "0.0.0",
  "private": false,
  "author": "Cossistant team",
  "description": "Headless React SDK for building AI-powered support/chat widgets. Hooks + primitives, WS-driven, TypeScript-first. Next.js-ready, Tailwind optional.",
  "keywords": [
    "react",
    "headless",
    "headless-ui",
    "support",
    "customer support",
    "helpdesk",
    "support widget",
    "chat widget",
    "live chat",
    "ai",
    "ai support",
    "ai-agent",
    "support agent",
    "typescript",
    "nextjs",
    "tailwind",
    "shadcn",
    "accessibility"
  ],
  "bugs": {
    "url": "https://github.com/cossistantcom/cossistant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cossistantcom/cossistant.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "homepage": "https://cossistant.com",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "import": "./dist/primitives/index.js"
    },
    "./primitives/*": {
      "types": "./dist/primitives/*.d.ts",
      "import": "./dist/primitives/*.js"
    },
    "./support": {
      "types": "./dist/support/index.d.ts",
      "import": "./dist/support/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./hooks/*": {
      "types": "./dist/hooks/*.d.ts",
      "import": "./dist/hooks/*.js"
    },
    "./support.css": "./dist/support.css",
    "./realtime": {
      "types": "./dist/realtime/index.d.ts",
      "import": "./dist/realtime/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cossistant/core": "workspace:*",
    "@cossistant/types": "workspace:*",
    "nanoid": "^5.1.5",
    "react-markdown": "^10.1.0",
    "react-use-websocket": "^4.13.0",
    "tailwind-merge": "^3.3.1",
    "ulid": "^3.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "postcss": "^8.5.6",
    "@cossistant/typescript-config": "workspace:*",
    "autoprefixer": "^10.4.21"
  },
  "peerDependencies": {
    "react": ">=18 <20",
    "react-dom": ">=18 <20",
    "@types/react": "^18 || ^19",
    "motion": "^12.18.1",
    "tailwindcss": "*"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "tsdown && bun run build:css",
    "build:css": "bunx @tailwindcss/cli -i src/support/support.css -o dist/support.css --content 'src/**/*.{js,ts,jsx,tsx}' --minify",
    "lint": "biome check .",
    "check-types": "tsc --noEmit --project tsconfig.json"
  }
}
